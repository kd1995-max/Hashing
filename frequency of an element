#include<bits/stdc++.h>
using namespace std;

int main(int argc, char const *argv[])
{
    int size;
    cin >> size;
    unordered_map<int, int> um;
    int buff;
    for (int i = 0; i < size; i++)
    {
        cin >> buff;
        um[buff]++;
    }
    
    for(auto a : um)
    cout << a.first << " " << a.second << endl;

    return 0;
}
